{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customDuotone": false,
			"defaultDuotone": false,
			"defaultPalette": true,
			"duotone": [],
			"palette": [
				{
					"color": "#D95E9D",
					"name": "Pink",
					"slug": "pink"
				},
				{
					"color": "#464EA3",
					"name": "Bleu",
					"slug": "blue"
				},
				{
					"color": "#EDEDED",
					"name": "Gris 1",
					"slug": "grey"
				},
				{
					"color": "#ADADAD",
					"name": "Gris 2",
					"slug": "grey2"
				},
				{
					"color": "#707070",
					"name": "Gris 3",
					"slug": "grey3"
				},
				{
					"color": "#4C4C4C",
					"name": "Gris 4",
					"slug": "grey4"
				}
			]
		},
		"layout": {
			"contentSize": "min(110rem,80%)",
			"wideSize": "100%"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "1/4",
					"size": "calc(.25 * var(--gap))",
					"slug": "quarter"
				},
				{
					"name": "1/2",
					"size": "calc(.5 * var(--gap))",
					"slug": "half"
				},
				{
					"name": "1",
					"size": "var(--gap)",
					"slug": "gap"
				},
				{
					"name": "2",
					"size": "calc( 2 * var(--gap))",
					"slug": "gap2"
				},
				{
					"name": "3",
					"size": "calc( 3 * var(--gap))",
					"slug": "gap3"
				},
				{
					"name": "4",
					"size": "calc( 4 * var(--gap))",
					"slug": "gap4"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "\"Inter\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "lighter",
							"src": [
								"file:./assets/fonts/inter-v13-latin_latin-ext-100.woff2"
							]
						},
						{
							"fontFamily": "\"Inter\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"src": [
								"file:./assets/fonts/inter-v13-latin_latin-ext-regular.woff2"
							]
						},
						{
							"fontFamily": "\"Inter\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/inter-v13-latin_latin-ext-800.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\", Helvetica Neue, Helvetica, Arial,  sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "calc( var(--fz) * .5)",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "calc( var(--fz) * .75)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "calc(var(--fz) * 1.25)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "calc( var(--fz) * 1.5)",
					"slug": "large"
				},
				{
					"name": "Very Large",
					"size": "calc( var(--fz) * 2)",
					"slug": "x-large"
				},
				{
					"name": "XXL",
					"size": "calc( var(--fz) * 3)",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "#fff",
			"text": "var(--wp--preset--color--black)"
		},
		"spacing": {
			"blockGap": "var(--gap)"
		},
		"typography": {
			"fontFamily": "\"Inter\", Helvetica Neue, Helvetica, Arial, sans-serif!important",
			"fontSize": "var(--fz)",
			"fontWeight": "normal"
		}
	},
	"version": 2
}